/**app.ttss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/* 对应图片创作页面的样式 */
.tab-item.page-scenarios {
  color: #999; /* 默认状态下的文字颜色 */
}
.tab-item.page-scenarios.active {
  color: #000; /* 选中状态下的文字颜色 */
}

/* 对应我的页面的样式 */
.tab-item.page-myinfo {
  color: #999; /* 默认状态下的文字颜色 */
}
.tab-item.page-myinfo.active {
  color: #000; /* 选中状态下的文字颜色 */
}